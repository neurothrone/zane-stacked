@page "/admin"
@layout AdminLayout
@using Microsoft.AspNetCore.Authorization
@attribute [Authorize]
@inject NavigationManager Navigation

<PageTitle>Admin - ZaneStacked</PageTitle>

<p>Welcome, Admin! You can manage skills and projects here.</p>

<button class="btn btn-danger" @onclick="Logout">Logout</button>

@code {

    private void Logout()
    {
        Navigation.NavigateTo("/logout");
    }

}